<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PersonDetailsLayout">
        <attr name="label" format="string|reference" />
        <attr name="value" format="string|reference" />
        <attr name="showLine" format="boolean" />
        <attr name="startIconResource" format="reference" />
        <attr name="endIconResource" format="reference" />
        <attr name="imeOptionType" format="enum">
            <enum name="actionNext" value="0" />
            <enum name="actionDone" value="1" />
        </attr>
        <attr name="fieldType" format="enum">
            <enum name="TEXT_ALL_CAPS" value="0" />
            <enum name="TEXT" value="1" />
            <enum name="NUMBER" value="2" />
            <enum name="EMAIL" value="3" />
        </attr>
        <attr name="isEnabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="DetailsAppSwitch">
        <attr name="appSwitchLabel" format="string|reference" />
        <attr name="appSwitchShowLine" format="boolean" />
    </declare-styleable>
    <declare-styleable name="MaterialSearchView">
        <attr name="searchHint" format="reference|string" />
        <attr name="secondaryIconResource" format="reference" />
    </declare-styleable>
</resources>